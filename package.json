  "main": "backend/ts/app.ts",
  "scripts": {
    "prod:frontend": "webpack --config ./frontend/webpack.config.prod.js",
    "prod:backend": "tsc --p ./backend/tsconfig.json",
    "prod": "run-s prod:**",
